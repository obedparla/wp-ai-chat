{
    "name": "wpaip/wp-ai-provider",
    "description": "Transparent OpenAI proxy server for WP AI Chatbot",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=8.2",
        "openai-php/client": "^0.10",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "yoast/phpunit-polyfills": "^3"
    },
    "autoload": {
        "psr-4": {
            "WPAIP\\": "includes/"
        }
    },
    "config": {
        "allow-plugins": {}
    },
    "scripts": {
        "test": "phpunit"
    }
}
